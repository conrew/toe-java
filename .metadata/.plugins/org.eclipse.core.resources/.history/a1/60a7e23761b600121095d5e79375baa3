package game;

public class test {

	public static void main(String[] args) {
		int l = 0;
		int l1 = 0;
		int l2 = 0;
		System.out.println("Generiere....");
		while(l < 2000){
		int i = (int)(Math.random()*2 + 1);
		switch(i){
		case 1:
			l1++;
			break;
			
		case 2:
			l2++;
			break;
		}
		System.out.println("Total: 1: " + l1 +  " 2: " + l2);
		l++;
		}
	}

}
